{
  "name": "inforePCTemplate",
  "version": "0.1.0",
  "author": "sj",
  "private": true,
  "scripts": {
    "start-dev": "better-npm-run start-dev --port 8099",
    "start-test": "better-npm-run start-test --port 8099",
    "start-k8s-uat": "better-npm-run start-k8s-uat --port 8099",
    "start-prod": "better-npm-run start-prod --port 8099",
    "build": "vue-cli-service build",
    "deploy-dev": "better-npm-run deploy-dev",
    "deploy-test": "better-npm-run deploy-test",
    "deploy-k8s-uat": "better-npm-run deploy-k8s-uat",
    "deploy-prod": "better-npm-run deploy-prod",
    "lint": "vue-cli-service lint"
  },
  "betterScripts": {
    "start-dev": {
      "command": "vue-cli-service serve",
      "env": {
        "API_ENV": "dev"
      }
    },
    "start-test": {
      "command": "vue-cli-service serve",
      "env": {
        "API_ENV": "test"
      }
    },
    "start-k8s-uat": {
      "command": "vue-cli-service serve",
      "env": {
        "API_ENV": "k8s-uat"
      }
    },
    "start-prod": {
      "command": "vue-cli-service serve",
      "env": {
        "API_ENV": "prod"
      }
    },
    "deploy-dev": {
      "command": "vue-cli-service build",
      "env": {
        "API_ENV": "dev"
      }
    },
    "deploy-test": {
      "command": "vue-cli-service build",
      "env": {
        "API_ENV": "test"
      }
    },
    "deploy-prod": {
      "command": "vue-cli-service build",
      "env": {
        "API_ENV": "prod"
      }
    },
    "deploy-k8s-uat": {
      "command": "vue-cli-service build",
      "env": {
        "API_ENV": "k8s-uat"
      }
    }
  },
  "dependencies": {
    "@types/js-cookie": "^2.2.7",
    "ant-design-vue": "^2.1.6",
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "js-cookie": "^3.0.1",
    "postcss-px2rem-exclude": "0.0.6",
    "pubsub-js": "^1.9.3",
    "px2rem-loader": "^0.1.9",
    "register-service-worker": "^1.7.1",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0-0",
    "vuex": "^4.0.0-0"
  },
  "devDependencies": {
    "@types/pubsub-js": "^1.8.2",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-pwa": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "babel-plugin-import": "^1.13.3",
    "better-npm-run": "^0.1.1",
    "compression-webpack-plugin": "^5.0.1",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "shelljs": "^0.8.4",
    "typescript": "~4.1.5"
  }
}
